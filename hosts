[kubernetes]

# works in ansible version 2.x.x (for key )
node1 ansible_host=REPLACE_HOST_IP ansible_user=<SSH_USER> ansible_ssh_private_key_file=</path/to/key/file >


# works for password based ssh in ansible version 2.x.x
# node1 ansible_host=REPLACE_HOST_IP ansible_user=<SSH_USER> ansible_ssh_pass=<YOURPASSWORD> ansible_become_pass=<YOURPASSWORD>
